
{
  "manifest_version": 2,
  "name": "TransVid",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "inject.js"]
    }
  ],
  "browser_action" : {
      "default_popup" : "popup.html",
      "default_icon" : "image/icon.png"
  },
  "permissions": [ "tabs" ]
}